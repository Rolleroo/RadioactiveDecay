{% extends "layout.jinja2" %}

{% block content %}
<div class="content">
  <table class="table" method="post">
    <thead>
      <th>First Column Name</th>
      <th>Second Column Name</th>
    </thead>
    <tbody>
      {% for result in results %}
      <tr>
        <td>{{ result.a }}</td>
        <td>{{ result.b }}</td>
      </tr>
      {% endfor %}
    </tbody>
  </table>
</div>
{% endblock content %}
